{
 azkar : [
   {                 "type": "رسالة",
            "zekr": "'بقلم شاعر وفرشة فنان اسبق الانس والطير والجان وابارك لكم قرب شهر رمضان..'"      },
   {                 "type": "رسالة",
            "zekr": "'حاسب نفسك قبل ان تحاسب'"      },
   {                 "type": "رسالة",
            "zekr": "'رمضان قرب عسى اجرك اقرب ياللي من قلبي اقرب وكل عام وانت للجنة اقرب..'"      },
   {                 "type": "رسالة",
            "zekr": "'من تعلم القرآن....عظمت قيمته\nو من تعلم الحديث.... قويت حجته\nو من تعلم الفقه .... نبل قدره\nو من تعلم اللغه .... رقّ طبعه\nو من تعلم الحساب ... زجل رأيه\nو من لم يصن نفسه ... لم ينفعه علمه'"      },
   {                 "type": "رسالة",
            "zekr": "'يا حمامه الحرم زوريه من زمزم أسقيه من عطر الكعبة طيبيه وبقرب رمضان هني'"      },
   {                 "type": "رسالة",
            "zekr": "'يارب البسمه تنور وجهك والفرحه تملا حياتك والايمان يعمر قلبك انت وكل من حولك'"      },
   {                 "type": "رسالة",
            "zekr": "'نور الله صدرك كلما افلت الشمس والقمر وازاح الله همك كلما حج فوج واعتمر وغفر الله لك ولوالديك على مد البصر وكثر احبابك ومحبيك فيه بعدد ملايين البشر'"      },
   {                 "type": "رسالة",
            "zekr": "'الحياه حلوه با الايمان'"      },
   {                 "type": "رسالة",
            "zekr": "'ادم الصلاة على الحبب محمد فقبولها دوما بدون تردد\nان شئتمو بعد الضلالة تهتدوا  صلوا على الغالى الحبيب محمد'"      },
   {                 "type": "رسالة",
            "zekr": "'قال صلى الله عليه وسلم(خير الناس من طال عمره وحسن عمله)'"      },
   {                 "type": "رسالة",
            "zekr": "'تأكد أن الحياة هم أتى من عصيان الأنس للخالق فلا تجعل همك لها وأنما أجعله للمفقود وهو الجنة'"      },
   {                 "type": "رسالة",
            "zekr": "'يارب البسمه تنور وجهك والفرحه تملاحياتك والايمان يعمرقلبك انت وكل من حولك'"      },
   {                 "type": "رسالة",
            "zekr": "'أعظم نعمة ينتظرها المسلم رؤية الله في الجنة'"      },
   {                 "type": "رسالة",
            "zekr": "'كأنني أرى دموعاً سالت على وجنتي العائدون إلى الله'"      },
   {                 "type": "رسالة",
            "zekr": "'من لزم الاستغفار جعل الله له من كل ضيق مخرجا ومن كل هم فرجا'"      },
   {                 "type": "رسالة",
            "zekr": "'يوم تحس بضيق وفراغ (ردد): لا إله إلا أنت سبحانك إني كنت من الضالمين'"      },
   {                 "type": "رسالة",
            "zekr": "'أروع القلوب قلب يخشى الله وأجمل الكلام ذكر الله وأنقى الحب الحب لله'"      },
   {                 "type": "رسالة",
            "zekr": "'أفضل نجاح يحققه الإنسان نجاح العتق من النار'"      },
   {                 "type": "رسالة",
            "zekr": "'الله يسعدك و للخير يرشدك و بالجنه يوعدني و يوعدك'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم أذق قلب أخي برد عفوك و حلاوة حبك و افتح مسامع قلبه لذكرك'"      },
   {                 "type": "رسالة",
            "zekr": "'من وجد الله فماذا فقد؟ومن فقد الله فماذا وجد؟'"      },
   {                 "type": "رسالة",
            "zekr": "'أهديكم دعوتين من قلب ما ينساكم الله يرعاكم وفي الجنة نلقاكم'"      },
   {                 "type": "رسالة",
            "zekr": "'سأل الذي جمعنا في دنيا فانية أن يجمعنا ثانية في جنة قطوفها دانية'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم لا سهل إلا ما جعلته سهلا وانت تجعل الحزن إذا شئت سهلا'"      },
   {                 "type": "رسالة",
            "zekr": "'أخي في الله إن اتقيت الله وأكثرت النوافل أحبك الله ثم الخلق'"      },
   {                 "type": "رسالة",
            "zekr": "'من قرأ سورة الدخان في ليله اصبح يستغفر له 70 الف ملك'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم حبب إلينا الإيمان وزينه فى قلوبنا'"      },
   {                 "type": "رسالة",
            "zekr": "'الدعاء نجى يونس وأهلك قوم نوح ورفع قدر سليمان وأظهر دين محمد فلا تتردد'"      },
   {                 "type": "رسالة",
            "zekr": "'أعمر فؤادك بالتقوي فالعمر محدود، واحمل مصحفا يشرح الصدور'"      },
   {                 "type": "رسالة",
            "zekr": "'ياابن ادام تذكر ان الدنيا دار ممر وليست دار مستقر بل هي دار عمل وتزود من الاعمال الصالحات رسائل دينية واعلم بانك في هذه الدنيا كعابر سبيل استظل بظل شجر ثم ارتحل وتركها خلفه فهذه الدنيا كمثل هذه الشجرة يارعاك الله'"      },
   {                 "type": "رسالة",
            "zekr": "'ألبسك اللــه حلل الصفــــاء وأنار لك الأرض والسمــــاء وجعل لك الفوز يوم اللقــاء وأسكنك منازل الشهــــــداء وهنأك ربي بعيش السـعداء'"      },
   {                 "type": "رسالة",
            "zekr": "'المسا بدونك ظلام والصمت بعيونك كلام؛ وألك مني يا غالي ألف تحية وسلام؛ يا أحلى قمر بليالي الغرام'"      },
   {                 "type": "رسالة",
            "zekr": "'مستحيل تشرق الشمس وماتضويك ومستحيل تشرق الشمس وما أهنيك'"      },
   {                 "type": "رسالة",
            "zekr": "'الهي لي احبة بالفؤاد ذكراهم …ولست دوما اراهم…فاسعدهم واجعل الجنة مأواهم'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم اني احب عبدك هذا حبا خالصا فيك…فارضه رضا لا يحزن بعده ابدا'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم أعنا على ذكرك وشكرك وحسن عبادتك'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم إني أعوذ بك من شر نفسي ومن شر الشيطان وشركه'"      },
   {                 "type": "رسالة",
            "zekr": "'يارب إن عظمت ذنوبي كثرة فلقد علمت بأن عفوك أعظم'"      },
   {                 "type": "رسالة",
            "zekr": "'أصبحنا وأصبح الملك لله والحمد لله أستغفرك وأتوب إليك'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم اغفر لي ذنبي ووسع في داري وبارك في رزقي'"      },
   {                 "type": "رسالة",
            "zekr": "'رب أعوذ بك من الكسل وسوء الكبر اللهم فقهني في الدين'"      },
   {                 "type": "رسالة",
            "zekr": "'ربنا أتتم لنا نورنا واغفر لنا إنك على كل شيء قدير'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم استر عوراتي وآمن روعاتي أعوذ بعظمتك أن أغتال من تحتي'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم لا تكلني إلى نفسي طرفة عين يا أرحم الراحمين'"      },
   {                 "type": "رسالة",
            "zekr": "'إلى أغلى أعز أحلى أحب الناس عيدك سعيد يا تاج الراس'"      },
   {                 "type": "رسالة",
            "zekr": "'عندما تبحث عن النور في زمن الظلمة أدعوك لقراءة سورة الكهف يوم الجمعة'"      },
   {                 "type": "رسالة",
            "zekr": "'قلبك اذا عطش فلا تسقه إلا بالقرآن .. واذا استوحش فلا تشغله إلا بالرحمن'"      },
   {                 "type": "رسالة",
            "zekr": "'يا حي يا قيوم .. ظلل أحبتي بالغيوم .. وأبعد عنهم الهموم .. وارزقهم عافية تدوم'"      },
   {                 "type": "رسالة",
            "zekr": "'ذكرك الله في الملا الاعلى بما تحبين .. وامنك مما تخافين  .. واسكنك منازل المتقين'"      },
   {                 "type": "رسالة",
            "zekr": "'هنأك الله بالقبول.. واسكنك الجنة مع الرسول.. وجعل جمعتك فاتحة بهجة لا تزول'"      },
   {                 "type": "رسالة",
            "zekr": "'عطر فؤادك بالتقى .. فالعمر محدود السنين .. واحمل بصدرك مصحفاً .. يشرح فؤادك كل حين'"      },
   {                 "type": "رسالة",
            "zekr": "'الهم افتح لمن فتح رسائل موبايل اسلامية باب السرور واجعل له فى قلبه نور و يسر له كل الأمور ..يا رب العالمين'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم هذا الغالى عينى لا تراه فعينك الهم ارعاه و فرج همه وشكواه واجل السعاده السعاده لا تفارق عيناه وبحكمتك يارب سدد خطاه وبرحمتك اجعل الجنه مثواه'"      },
   {                 "type": "رسالة",
            "zekr": "'يا خالق الاكوان يارازق الانسان اجعل اللهم مثوانا اعلى الجنان'"      },
   {                 "type": "رسالة",
            "zekr": "'بحق من انزل سوره فاطر ان غبت عن العين لا تغيب عن الخاطر'"      },
   {                 "type": "رسالة",
            "zekr": "'انت اصلا لاتستحق لا رساله ولا رنه انت تستحق دعوه بالجنه'"      },
   {                 "type": "رسالة",
            "zekr": "'اسأل رب الارض و السموات ان يدخلنا الجنه مع الذاكرين الله كثيرا و الذاكرات'"      },
   {                 "type": "رسالة",
            "zekr": "'صبحك الله بالسعادة ..\nورطب لسانك بالشهادة ..\nوحـ.ــبب فيك خلقه وسخر لك عباده ..\nوجعل خير عمرك آخره وخير عملك خواتمه وخير\nأيامك يوم لقائه ..'"      },
   {                 "type": "رسالة",
            "zekr": "'يوم تحس بضيق وفراغ (ردد): لا إله إلا أنت سبحانك إني كنت من الضالمين'"      },
   {                 "type": "رسالة",
            "zekr": "'اللهم اجعل لغاليتي دعوة لا تـُرَد ..\nوهبها رزقا لا يُعَـد ..\nوافتح لها بابا إلى الجنة لا يُسَـد ..\nفإني أحبها فيك بغير حَد ..'"      },
   {                 "type": "رسالة",
